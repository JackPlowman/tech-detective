{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "additionalProperties": false,
  "type": "object",
  "properties": {
    "repositories": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "full_name": {
              "type": "string"
            },
            "project_technologies_and_frameworks": {
              "type": "array"
            }
          },
          "required": ["full_name", "project_technologies_and_frameworks"]
        }
      ]
    }
  },
  "required": ["repositories"]
}
